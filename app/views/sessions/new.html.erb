<h1>Log In </h1>

<%= form_for :login do |f| %>
	<div>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>

	<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>

	<%= f.submit "Log In" %>

	<p>Not a member? <%= link_to "Join TennisAnyone", signup_path %></p>

<% end %>